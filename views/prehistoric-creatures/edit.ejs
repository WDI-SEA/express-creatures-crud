<form method="POST" action="/prehistoric-creatures/<%= creatureId %>/?_method=PUT">
  <label for="type">Type</label>
  <input type="text" value="<%= creature.type %>" name="type">

  <label>Image</label>
  <input type="image" value="<%= creature.img %>" name="img">

  <input type="submit">
</form>

