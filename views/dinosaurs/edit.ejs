<form method="POST" action="/dinosaurs/<%= dinoId %>/?_method=PUT">
    <input type="text" value="<%= dino.name %>" name="name"/>
    <input type="text" value="<%= dino.type %>" name="type"/>
    <div id="submit-box">
        <input class="submit-button" type="submit" value="CHANGE!"/>
    </div>
</form>