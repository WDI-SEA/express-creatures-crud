<ul>
    <% dinoData.forEach( (dino, index) => { %>
      <li class="animalList">
        <%= dino.name %> is a <%= dino.type %>
        <a href="/dinosaurs/edit/<%= index %>">Edit dino</a>

        <form method="POST" action="/dinosaurs/<%= index %>/?_method=DELETE">
          <input type="submit" value="delete">
        </form>
       
      </li>
    <% }) %>
  </ul>