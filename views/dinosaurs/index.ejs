<h1>Dinosaurium</h1>
<ul>
    <% dinoData.forEach((dino, index) => { %>
        <li>
            <img class="list-icon" src="https://i.pinimg.com/originals/3b/e4/6c/3be46c121d65aeccc1e25ce14b9dd4b3.png"/>
            <%= dino.name %> is a <%= dino.type %>.
            <img class="list-icon" src="https://i.pinimg.com/originals/3b/e4/6c/3be46c121d65aeccc1e25ce14b9dd4b3.png"/>
                <div id="button-box">
                    <form action="/dinosaurs/dinosaurs/edit/<%= index %>">
                        <input class="submit-button" type="submit" value="EDIT"/>
                    </form>
                    <form method="POST" action="/dinosaurs/dinosaurs/<%= index %>/?_method=DELETE">
                            <input class="submit-button" type="submit" value="DELETE"/>
                    </form>
                </div>
        </li>
    <% }) %> 
</ul>
<a href="/dinosaurs/dinosaurs/new">
    <button class="new-button">NEW DINOSAUR</button>
</a>